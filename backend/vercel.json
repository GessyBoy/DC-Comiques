{
    "version": 2,
    "builds": [
      {
        "src": "backend/package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["backend/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/backend/$1"
      }
    ]
  }
  