{
    "version": 2,
    "routes": [
      {
        "src": "/index.js",
        "headers": { "Content-Type": "text/javascript" },
        "dest": "/index.js"
      },
      {
        "src": "./public/assets/css/style.css",
        "headers": { "Content-Type": "text/css" },
        "dest": "/public/assets/css/style.css"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  